<% @list.summaries.each do |s| %>
    Vue.component("summary-<%=s.id%>", {
      props: ["citedRefs"],
      template: "#s-<%=s.id%>",
      methods: {
        r: function(id) {
          return this.citedRefs.filter(function(r){
            return r.id == id;
          })[0]
        }
      }
    })
<% end %>
